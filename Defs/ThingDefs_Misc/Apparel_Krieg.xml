      <layers>
        <li>Overhead</li>
      </layers>
      <forceEyesVisibleForRotations>
        <li>1</li>
        <li>2</li>
        <li>3</li>
      </forceEyesVisibleForRotations>
	  <!--2--><useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
