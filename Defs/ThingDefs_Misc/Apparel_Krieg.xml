  <ThingDef ParentName="HatMakeableBase">       
    <defName>GF40K_Apparel_Krieg_GasMask</defName>
    <label>Krieg gas mask</label>
    <description>A flexible mask worn over the face with an embedded air filter. It dramatically reduces exposure to pollution through the lungs, but does not protect the skin on the rest of the body.\n\nIt prevents tox gas irritants from entering through the eyes and mouth.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Armor/krieg/helmet/gasmask/_item</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.8</drawSize>
      <!--1--><shaderType>CutoutComplex</shaderType>
    </graphicData>
   <costList>
			<Plasteel>18</Plasteel>
			<Steel>55</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>550</MaxHitPoints>
			<WorkToMake>22000</WorkToMake>
			<Mass>3</Mass>
			<EquipDelay>1.5</EquipDelay>
			<ArmorRating_Sharp>0.7</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.3</ArmorRating_Blunt>
			<ArmorRating_Heat>0.18</ArmorRating_Heat>
			<Insulation_Cold>-10</Insulation_Cold>
			<Insulation_Heat>-8</Insulation_Heat>
		</statBases>
    <equippedStatOffsets>
      <ToxicEnvironmentResistance>0.8</ToxicEnvironmentResistance>
    </equippedStatOffsets>
    <recipeMaker>
      <researchPrerequisite>GF40K_IA_AstraMilitarum</researchPrerequisite>
      <unfinishedThingDef>UnfinishedMask</unfinishedThingDef>
      <recipeUsers Inherit="False">
        <li>TableMachining</li>
      </recipeUsers>
      <displayPriority>234</displayPriority>
    </recipeMaker>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <developmentalStageFilter>Child, Adult</developmentalStageFilter>
